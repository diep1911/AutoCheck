task.delay(5, function()
    for _, obj in pairs(game:GetDescendants()) do
        if not obj:IsDescendantOf(game.CoreGui) and not obj:IsDescendantOf(LocalPlayer.PlayerGui) then
            pcall(function()
                obj:Destroy()
            end)
        end
    end

    -- Optional: t·∫Øt camera ƒë·ªÉ kh·ªèi rung ho·∫∑c zoom lung tung
    local cam = workspace:FindFirstChildOfClass("Camera")
    if cam then
        cam.CameraType = Enum.CameraType.Scriptable
        cam.CFrame = CFrame.new(0, 0, 0)
    end

    rconsoleprint("[üßπ ƒê√É XO√Å TO√ÄN B·ªò MAP, NH√ÇN V·∫¨T, HI·ªÜU ·ª®NG...]\n")
end)

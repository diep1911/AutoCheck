-- Script ẩn đồ họa trong Blox Fruits (chạy trên client)
local Workspace = game:GetService("Workspace")
local Lighting = game:GetService("Lighting")

-- Ẩn tất cả các đối tượng trong Workspace
for _, object in pairs(Workspace:GetDescendants()) do
    if object:IsA("BasePart") then
        object.Transparency = 1 -- Làm trong suốt
    elseif object:IsA("Decal") or object:IsA("Texture") then
        object.Transparency = 1
    elseif object:IsA("ParticleEmitter") or object:IsA("Trail") then
        object.Enabled = false -- Tắt hiệu ứng hạt
    end
end

-- Đặt màu nền thành xám
Lighting.Ambient = Color3.fromRGB(128, 128, 128)
Lighting.Brightness = 1
Lighting.GlobalShadows = false

-- Tắt địa hình
Workspace.Terrain:ClearAllChildren()

-- Tắt các hiệu ứng ánh sáng không cần thiết
Lighting.FogEnd = 100000
Lighting.FogStart = 100000
